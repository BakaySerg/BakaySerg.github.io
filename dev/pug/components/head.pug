head
	// Пути для css, js и изображений
	- path = {css: 'assets/css', js: 'assets/js', img: 'assets/img'};
	// meta 
	meta(charset="UTF-8")
	title S-pro-test (Bakay Sergey)
	meta(name="description", content="")
	meta(http-equiv="X-UA-Compatible", content="IE=edge")
	meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
	meta(property="og:image", content = path.img + "/image.jpg")
	// favicon
	link(rel="shortcut icon", href = path.img + "/favicon/favicon.png", type="image/x-icon")
	link(rel="apple-touch-icon", sizes="180x180", href = path.img + "/favicon/apple-touch-icon-180x180.png")
	// theme
	meta(name="theme-color" content="#2998cc")
	meta(name="msapplication-navbutton-color" content="#2998cc")
	meta(name="apple-mobile-web-app-status-bar-style" content="#2998cc")
	// links
	link(rel="stylesheet", href = path.css + "/main.min.css")

	

